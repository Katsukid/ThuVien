
<yazgfs>
<!-- [scheme:]host[:port][/databaseName] -->
<!-- scheme: tcp, ssl, unix, http, sru -->
<!-- can run all servers on tcp, but the unix socket is faster -->

<listen id="biblioserver" >unix:/build/koha-23.05.04/debian/tmp/var/run/koha/zebradb/bibliosocket</listen>
<listen id="authorityserver" >unix:/build/koha-23.05.04/debian/tmp/var/run/koha/zebradb/authoritysocket</listen>

<!-- Uncomment the following entry if you want to run the public Z39.50 server.
    Also uncomment the <server> and <serverinfo> sections for id 'publicserver' 
    under PUBLICSERVER'S BIBLIOGRAPHIC RECORDS title-->
<!--
<listen id="publicserver" >tcp:@:9998</listen>
-->

<!-- BIBLIOGRAPHIC RECORDS -->
<server id="biblioserver"  listenref="biblioserver"> 
    <directory>/build/koha-23.05.04/debian/tmp/var/lib/koha/zebradb/biblios</directory>
    <config>/build/koha-23.05.04/debian/tmp/etc/koha/zebradb/zebra-biblios-dom.cfg</config>
    <cql2rpn>/build/koha-23.05.04/debian/tmp/etc/koha/zebradb/pqf.properties</cql2rpn>
    <xi:include href="/build/koha-23.05.04/debian/tmp/etc/koha/zebradb/retrieval-info-bib-dom.xml" xmlns:xi="http://www.w3.org/2001/XInclude"/>
    <xi:include href="/build/koha-23.05.04/debian/tmp/etc/koha/zebradb/explain-biblios.xml" xmlns:xi="http://www.w3.org/2001/XInclude"/>
</server>
<serverinfo id="biblioserver">
        <ccl2rpn>/build/koha-23.05.04/debian/tmp/etc/koha/zebradb/ccl.properties</ccl2rpn>
        <user>kohauser</user>    
        <password>zebrastripes</password>
</serverinfo>

<!-- AUTHORITY RECORDS -->
<server id="authorityserver"  listenref="authorityserver" >
    <directory>/build/koha-23.05.04/debian/tmp/var/lib/koha/zebradb/authorities</directory>
    <config>/build/koha-23.05.04/debian/tmp/etc/koha/zebradb/zebra-authorities-dom.cfg</config>
    <cql2rpn>/build/koha-23.05.04/debian/tmp/etc/koha/zebradb/pqf.properties</cql2rpn>
    <xi:include href="/build/koha-23.05.04/debian/tmp/etc/koha/zebradb/retrieval-info-auth-dom.xml" xmlns:xi="http://www.w3.org/2001/XInclude"/>
    <xi:include href="/build/koha-23.05.04/debian/tmp/etc/koha/zebradb/explain-authorities.xml" xmlns:xi="http://www.w3.org/2001/XInclude"/>
</server>
<serverinfo id="authorityserver">
    <ccl2rpn>/build/koha-23.05.04/debian/tmp/etc/koha/zebradb/ccl.properties</ccl2rpn>
    <user>kohauser</user>
    <password>zebrastripes</password>
</serverinfo>

